      "bin": "./kubectl-custom-cols",
      "files": [
        {
          "from": "./kubectl-custom-cols-*/kubectl-custom-cols",
          "to": "."
        },
        {
          "from": "./kubectl-custom-cols-*/LICENSE",
          "to": "."
